          <argument>
            <kind>1</kind>
            <string>///Increment meter
scr_getinputs();

switch (state)
{
    case m_states.open:
    {
        if ((mouse_right_p) &amp;&amp; (meter &gt;= 1))
        {
            global.smod = 0.5;
            meter--;
        }
        else
        {
            global.smod = 1;
            meter = meter + 4;
        }
        if (meter == 0) state = m_states.charging;
    }
    break;
    
    case m_states.charging:
    {
        global.smod = 1;
        meter = meter + 4;
        if (meter == 320) state = m_states.open;
    }
    break;
}
    

meter = max(0, meter);
meter = min(meter, 320);
</string>
