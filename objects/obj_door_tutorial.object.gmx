            <string>///Get inputs, check collision, spawn text, move player
scr_getinputs();
//Check text collision
if (place_meeting(x, y, obj_player) &amp;&amp; image_index == 0) collision = 1;
else
{
    collision = 0;
    i = 0;
}

//Lock or unlock door based on x position
switch(x)
{
    case 1088: image_index = 0;
    break;
    
    case 2640: image_index = 0;
    break;
    
    case 3952: 
    if (!collision_rectangle(3824, 400, 4016, 560, obj_target_tutorial, false, false)) image_index = 0;
    else image_index = 1;
    break;
    
    case 5232: 
    if (!instance_exists(obj_spawner_tutorial_moving)) image_index = 0;
    else image_index = 1;
    break;
    
    case 6832: 
    if (!instance_exists(obj_spawner_tutorial_enemy)) image_index = 0;
    else image_index = 1;
    break;
    
    //case : image_index = 1;
    //break;
}

//Teleport player based on doorpos
if (place_meeting(x, y, obj_player) &amp;&amp; key_jump &amp;&amp; image_index == 0)
{
    global.doorpos++;
    switch (global.doorpos)
    {
        case 0: break;
        
        case 1:
        obj_player.x = 1952;
        obj_player.y = 624;
        break;
        
        case 2:
        obj_player.x = 3488;
        obj_player.y = 624;
        break;
        
        case 3:
        instance_create(x, y, obj_slowmo_tutorial);
        instance_create(x, y, obj_spawner_tutorial_moving);
        obj_player.x = 4800;
        obj_player.y = 624;
        break;
        
        case 4:
        instance_create(x, y, obj_spawner_tutorial_enemy);
        instance_create(x, y, obj_smg_box_spawner);
        instance_create(x, y, obj_combo_tutorial);
        obj_player.x = 6080;
        obj_player.y = 624;
        break;
        
        case 5:
        instance_create(8672, 576, obj_enemy_tutorial);
        instance_create(8288, 528, obj_box_tutorial_revolver);
        obj_player.x = 7712;
        obj_player.y = 576;
        break;
    }
}
