    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///State machine, collision, gun position

//Position gun
gun.x = x;
gun.y = y;

//State machine
if (instance_exists(obj_player))
{
    switch (state)
    {
        case e_states.chase:
        {
            dir = sign(obj_player.x - x);
            movespeed = global.speed_mod;
            var los = !collision_line(x, y, obj_player.x, obj_player.y, obj_wall, false, true);
            var rad = (distance_to_object(obj_player) &lt; radius)
            if (los &amp;&amp; rad) state = e_states.stop;
            if (place_meeting(x+hsp+1,round(y),obj_wall) || place_meeting(x+hsp-1,round(y),obj_wall)) jump = 1;
            else jump = 0;
            with (gun)
            {
                move_towards_point(x + other.dir, y, 0);
                image_angle = direction;
                var xbuff = lengthdir_x(40, direction);
                var ybuff = lengthdir_y(40, direction);
                x += xbuff;
                y += ybuff;
            }
        }
        break;
        
        case e_states.stop:
        {
            with (gun)
            {
                move_towards_point(obj_tracker.x, obj_tracker.y, 0);
                image_angle = direction;
                var xbuff = lengthdir_x(40, direction);
                var ybuff = lengthdir_y(40, direction);
                x += xbuff;
                y += ybuff;
            }
            jump = 0;
            movespeed = 0.1;
            scr_enemyaim();
        }
        break;
        
        case e_states.aim:
        {
            with (gun)
            {
                move_towards_point(obj_tracker.x, obj_tracker.y, 0);
                image_angle = direction;
                var xbuff = lengthdir_x(40, direction);
                var ybuff = lengthdir_y(40, direction);
                x += xbuff;
                y += ybuff;
            }
            
            if (instance_exists(circle))
            {
                circle.x = gun.x;
                circle.y = gun.y;
            }
            
            if (aimtimer &gt; 0) aimtimer = aimtimer - (1 * global.smod);
            else if (aimtimer &lt;= 0)
            {
                scr_enemyshoot();
                with (circle) instance_destroy();
            }
        }
        break;
    }
    scr_enemycollideandmove();
}

else
{
    if instance_exists(circle) with(circle) instance_destroy();
    movespeed = global.speed_mod;
    with (gun)
    {
        move_towards_point(x + other.dir, y, 0);
        image_angle = direction;
        var xbuff = lengthdir_x(40, direction);
        var ybuff = lengthdir_y(40, direction);
        x += xbuff;
        y += ybuff;
    }
    scr_enemycollideandmove_dead();
}
</string>
